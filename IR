  function IntimationDeath(){
  	deathdate=window.document.all("Death_dt").value
  	if (deathdate!=""){
  		$("#div8").css("display","block")
  		window.document.all("IntimationDeathSpan").style.display="inline-block";
  		window.document.all("calendar_input08").className="fieldinput_mandatory";
  		window.document.all("Text081").className="fieldinput_mandatory";
  		window.document.all("Text082").className="fieldinput_mandatory";
  		window.document.all("Text083").className="fieldinput_mandatory"
  	}
  }
  function bogusertype()
    {
    	var usertype  
        try
        {
        	usertype=window.frameElement.document.body.GetUserInfo("UserType");
        }
        catch(e) 
        {
            usertype=window.dialogArguments.parentElement.GetUserInfo("UserType");
        }
        if(usertype == 5) 
        {
            deathdate=window.document.all("Death_dt").value
            
  			if (deathdate==""){
  				$("#div8").css("display","none")
  				window.document.all("IntimationDeathSpan").style.display="none";
  				window.document.all("calendar_input08").className="AutoComplete_nonMandatory";
  				window.document.all("Text081").className="AutoComplete_nonMandatory";
  				window.document.all("Text082").className="AutoComplete_nonMandatory";
  				window.document.all("Text083").className="AutoComplete_nonMandatory"
  				window.document.all("Intimate_Death_dt").value=""
  				}
        }
        
    }
    
    function validintimatedeath(){
            deathdate=window.document.all("Death_dt").value
            deathdate1=commondate(deathdate)
            deathdate2=Number(deathdate1)
            intimatedate=window.document.all("Intimate_Death_dt").value
            intimatedate1=commondate(intimatedate)
            intimatedate2=Number(intimatedate1)
            if (deathdate2==0){
  				alert("not a valid date")
  				window.document.all("Death_dt").focus();
  				return false;
  				}
  			else if(intimatedate2==0){
  				alert("not a valid date");
  				window.document.all("Intimate_Death_dt").focus();
  				return false;
  			}
  			else{ 
  				return true;
  			}

    }

  function commondate(hdate5)
	{
		var arrDate8=hdate5.split("/");
		var dd8=arrDate8[0];
		var mm8=arrDate8[1];
		var yyyy8=arrDate8[2];
		var hostdate8 = yyyy8 + mm8 + dd8;
		return hostdate8;
	}
  function checkIntimationDeath1(){
  	deathdate=window.document.all("Death_dt").value
  	intimatedate=window.document.all("Intimate_Death_dt").value
  	todaydate=new Date();
	try
        {
            var hdate5 = window.frameElement.document.body.GetUserInfo("HostDate");
        }
    catch(e)
        {
            var hdate5 =window.dialogArguments.parentElement.GetUserInfo("HostDate");
        }
  	deathdt=commondate(deathdate)
  	intimatedt=commondate(intimatedate)
  	todaydt=commondate(hdate5)
  	if(deathdate!=""){
  		if (intimatedate!=""){
  			if(intimatedt<=todaydt){
  				return true;
  			}
  			else{
  				alert("Intimation of Death cannot be a future date")
  				window.document.all("Intimate_Death_dt").focus();
  				return false;
  			}
  		}
  		else{
  			alert("Please enter the date of Intimation of Death")
  			window.document.all("Intimate_Death_dt").focus();
  			return false; 			
  		}
  	}
  	else{
  		if (intimatedate!=""){
  			window.document.all("Death_dt").focus();
  			return false;
  		}
  		else{
  			return true;
  		}
  		
  	}


  }
  function checkIntimationDeath(){
  	deathdate=window.document.all("Death_dt").value
  	intimatedate=window.document.all("Intimate_Death_dt").value
  	deathdt=commondate(deathdate)
  	intimatedt=commondate(intimatedate)
  	if(deathdate!=""){
  		if (intimatedate!=""){
  			if(intimatedt>=deathdt){
  				return true;
  			}
  			else{
  				alert("Intimation of Death cannot be prior to Date of Death")
  				window.document.all("Intimate_Death_dt").focus();
  				return false;
  				
  			}
  		}
  		else{
  			alert("Please enter the date of Intimation of Death")
  			window.document.all("Intimate_Death_dt").focus();
  			return false;
  			
  		}
  	}
  	else{
  		if (intimatedate!=""){
  			window.document.all("Death_dt").focus();
  			return false;
  		}
  		else{
  			return true;
  		}
  	}

  }
// End of IR 24020013

//Start of CR 24020168
  $("#DefInteger33").on("autocompleteselect",hideValidate);

function hideValidate()
{
	

	if(window.document.all("DefInteger33").value=="99:ILLITERATE")
	{
		window.document.all("validateBtn").style.display="inline";
		return true;
	}
	else
	{
		window.document.all("validateBtn").style.display="none";
		return true;
	}
}   

function disableTransmit()
{
	alert(window.document.all("sendFlag").value);
	if(window.document.all("sendFlag").value == "Y")
	{
		window.document.all("Transmit").disabled=true;
		return true;
	}
  
}
