		//Start of IR 76016 of HOTFIX IR 76053
		function checkGenderU(){
			var genderCode = window.document.all("DefaultString2").value;

			if(genderCode =="U" )
			{
				alert("Please select Gender as T:Third Gender");
				window.document.all("DefaultString2").focus();
				return false;
			}
			return true;
		}
