// start of IR 21100098
function drop()
{
	if (window.document.all("Death_dt").value != "" && window.document.all("causeofdeath").value == "")
    {
                 			alert("Please Select Cause of Death");
                 			window.document.all("causeofdeath").focus();
                 			return false;
    }
    else if (window.document.all("Death_dt").value == "" && window.document.all("causeofdeath").value != "")
    {
                 		 	alert("Cause of Death Cannot Be Captured without Capturing Date of Death");
                 		 	window.document.all("Death_dt").focus();
                 		 	return false;
    }
    // Start of IR 23030101
    else if (window.document.all("DefInteger33").value == "")
    {
    								alert("You must select a value in Educational Qualification");
               			window.document.all("DefInteger33").focus();
               			return false;
    }
    // end of IR 23030101
    else 
    {
    	return true;
    }


}
//end of IR 21100098
//Start of IR 22020030
function onloadocpn()
{
	if (window.document.getElementById("ocpn_code").value == "0999:Not Categorized" || window.document.getElementById("ocpn_code").datavalue=="0999")
	{
		window.document.getElementById("ocpn_code").value="0999:";
        window.document.getElementById("ocpn_code").focus();
	}
    else{
      return true;
    }
}
function checkmartialstatus()
  {
    if(window.document.all("DefaultString3").value==""){
      alert("You must enter a value in Marital Status")
      window.document.getElementById("DefaultString3").focus();
      return false;
    }
	else if (window.document.getElementById("ocpn_code").value == "0999:Not Categorized" || window.document.getElementById("ocpn_code").datavalue=="0999" || window.document.getElementById("ocpn_code").value=="0999:")
	{
	window.document.getElementById("ocpn_code").value="0999:";
	alert("You must select a proper value in Occupation Subtype");
      window.document.getElementById("ocpn_code").focus();
      return false;
	}
    else{
      return true;
    }
  }
function occupationsubtypeempty(){
	document.getElementById('ocpn_code').value='';
}
