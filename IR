function SetValues()
{
        var date1,date2,date3;
		date1 = $("#calendar_input01").val();
		date2 = $("#calendar_input02").val();
		date3 = $("#calendar_input03").val();
		// Start of IR 24020013
		var date8;
		date8 = $("#calendar_input08").val();
		// End of IR 24020013
		if(date1.length != 0)
		{
			$("#Text011").val(date1.substring(0,2));
			$("#Text012").val(date1.substring(3,5));
			$("#Text013").val(date1.substring(6,10));
		}
		if(date2.length != 0){
			$("#Text021").val(date2.substring(0,2));
			$("#Text022").val(date2.substring(3,5));
			$("#Text023").val(date2.substring(6,10));
		}
		// Start of IR 24020013
