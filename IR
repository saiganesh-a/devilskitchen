//Start of IR 20010066
	function usertype()
    {
    	var usertype  
        try
        {
        	usertype=window.frameElement.document.body.GetUserInfo("UserType");
        }
        catch(e) 
        {
            usertype=window.dialogArguments.parentElement.GetUserInfo("UserType");
        }
        if(usertype == 11) 
        {
            alert ("Only Enquiry is allowed for Logged-in User")
            return false;
        }
        else 
        {
            return true;
        }
    }
