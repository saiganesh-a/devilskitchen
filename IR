				function ValidDOB()
				{
					var dob = window.document.all("date").value.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g,'').replace(/\s+/g,' ');

					if(dob !="")
					{
						var hodate
						try
						{
							hodate = window.frameElement.document.body.GetUserInfo("HostDate");
						}
						catch (e)
				                                    		{
							try
							{
								hodate = window.dialogArguments.parentElement.GetUserInfo("HostDate");
							}
							catch (e)
							{
								hodate = new Date();
								var dd= hodate.getDate();
								var mm=hodate.getMonth();
								var yyyy=hodate.getFullYear();
								hodate=dd+"/"+mm+"/"+yyyy;
							}
						}
						
						var arrDate2=hodate.split("/")
						var dd2=arrDate2[0]
						var mm2=arrDate2[1]
						var yyyy2=arrDate2[2]
						var hostdate1 = yyyy2 + mm2 + dd2;

						var TempExpDate = window.document.all("date").value;

						var temparray = TempExpDate.split("/");

						if(temparray.length==3 && !isNaN(temparray[2]) && !isNaN(temparray[1]) && !isNaN(temparray[0]))
						{
						  if(TempExpDate.length==10)
						{
							var leapYear=temparray[2]
							if (leapYear%400==0)
							{
								result = true
							}
							else if(leapYear%100==0)
							{
								result = false
							}
							else if(leapYear%4==0)
							{
								result= true
							}
							else
							{
								result= false
							}
							if(result==false)
							{
								if(temparray[1]=='02')
								{
									if(temparray[0]>28)
									{
										alert("Invalid Day in Date of Birth.");
										return false;
									}
								}
							}
							if(result==true)
							{
								if(temparray[1]=='02')
								{
									if(temparray[0]>29)
									{
										alert("Invalid Day in Date of Birth.");
										return false;
									}
								}
							}
							if(temparray[1]=='01' || temparray[1]=='03'|| temparray[1]=='05' || temparray[1]=='07' || temparray[1]=='08' || temparray[1]=='10' || temparray[1]=='12' )
							{
								if(temparray[0]>31)
								{
									alert("Invalid Day in Date of Birth.");
									return false;
								}
							}
							if(temparray[1]=='04' || temparray[1]=='06'|| temparray[1]=='09' || temparray[1]=='11')
							{
								if(temparray[0]>30)
								{
									alert("Invalid Day in Date of Birth.");
									return false;
								}
							}

			TempExpDate1 = temparray[2] + temparray[1] + temparray[0];
			if(temparray[0]<1 ||temparray[0]>31)
			{
				alert("Invalid Day in Date of Birth.");
				return false;
			}
			if(temparray[1]<1 ||temparray[1]>12)
			{
				alert("Invalid Month in Date of Birth.");
				return false;
			}
			var h=Number(hostdate1)
			var t=Number(TempExpDate1)
			if(h < t)
			{
				alert("Date of Birth cannot be a future date.");
				return false;
			}
			if(temparray[2]<1900)
			{
				alert("Birth year cannot be before 1900.");
				return false;
			}
			return true;
		}
		else
		{	
					 alert("Date of Birth is not in DD/MM/YYYY Format.");
					 return false;
	          }
		}
		else
		{	
			alert("Not a Valid Date of Birth.");
			return false;
		}
	}	
	else
	{	
		alert("Date of Birth cannot be empty.")
		return false;
	}
}

