					function checkCspFlag()
                            {								
								if (window.document.all("Csp_Flag").value =='Y')
								{
									 if(window.document.all("Transmit").disabled!=true)
										{	

											return true
					
										}
		
								}
								else
								{
									if(window.document.all("Transmit").disabled!=true)
										{	

											return true

										}

								}
				        }
